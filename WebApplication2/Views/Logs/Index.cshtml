@model IEnumerable<WebApplication2.Models.Logs>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.logDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.logThread)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.logLevel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.logSource)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.logMessage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.exception)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.logDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logThread)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logLevel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logSource)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logMessage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.exception)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
